{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/windows",
      "dest": "/windows.html"
    },
    {
      "src": "/lawn-care",
      "dest": "/lawn-care.html"
    },
    {
      "src": "/leaf-removal",
      "dest": "/leaf-removal.html"
    },
    {
      "src": "/spring-cleanup",
      "dest": "/spring-cleanup.html"
    },
    {
      "src": "/snow-removal",
      "dest": "/snow-removal.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "handle": "filesystem"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}